<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>checkLogin</servlet-name>
    <servlet-class>controller.checkLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Gimg</servlet-name>
    <servlet-class>controller.Gimg</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>checkvcode</servlet-name>
    <servlet-class>controller.checkvcode</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>controller.test</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>checkLogin</servlet-name>
    <url-pattern>/checkLogin.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Gimg</servlet-name>
    <url-pattern>/Gimg.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>checkvcode</servlet-name>
    <url-pattern>/checkvcode.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test.do</url-pattern>
  </servlet-mapping>
</web-app>